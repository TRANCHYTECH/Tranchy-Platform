        "type": "object",
        "properties": {
          "eventId": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateQuestionEventStatusChangedRequest": {
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/CreateQuestionEventRequest"
          }
        ],
